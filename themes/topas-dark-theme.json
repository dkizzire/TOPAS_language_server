	"semanticTokenColors": {
	  "variable": {
		"foreground": "#9CDCFE"  
	  },
	  "function": {
		"foreground": "#DCDCAA"  
	  },
	  "parameter": {
		"foreground": "#4EC9B0" 
	  },
	  "keyword": {
		"foreground": "#569CD6"  
	  },
	  "operator": {
		"foreground": "#D4D4D4"  
	  },
	  "macro": {
		"foreground": "#C586C0" 
	  }
	}
